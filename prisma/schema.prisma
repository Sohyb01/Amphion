// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  name String
  id String @id @default(uuid()) 
  price Float @default(0)
  discount Float
  battery_life Int
  img String?
}